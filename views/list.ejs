<!DOCTYPE html>
<html lang="ko">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>거래내역</title>
</head>
<body>
  <% if(is_logined==false){ %>
    <table width = "30%">
      <tr>
        <td><a href="/">Home</a></td>
        <td><a href="/login">로그인</a></td>
        <td><a href="/register">회원가입</a></td>
      </tr>
    </table>
    <% }else{ %>
    <table width = 30%>
      <tr>
        <td><a href="/index/<%= id %>">Home</a></td>
        <td><a href="/list/<%= id %>">거래내역</a></td>
        <td>
          <%= name %> 님 환영합니다!
        </td>
        <td><a href="/mypage/<%= id %>">마이페이지</a></td>
        <td><a href="/logout">로그아웃</a></td>
      </tr>
    </table>
    <% } %>

    <table>
      <tr>
        <th>계좌번호</th>
        <th>기관</th>
        <th>예금잔액</th>
        <th>계좌삭제</th>
      </tr>
      <% account.forEach(function (value) { %>
        <tr>
          <td><%= aNUMBER %></td>
          <td><%= bank %></td>
          <td><a href="/remain/<%= value.aNUMBER %>/<%= id %>">조회</a></td>
          <td><a href="/delete/<%= value.aNUMBER %>/<%= id %>">삭제</a></td>
        </tr>
        <% }); %>
    </table>
  
</body>
</html>